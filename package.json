        "@types/node": "^18.11.18",
        "@weichwarenprojekt/wersion": "^0.1.1",
        "jest": "^29.4.1",
        "prettier": "^2.8.1",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
