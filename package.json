    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Weichwarenprojekt/wp-license-reporter/issues"
    },
    "homepage": "https://github.com/Weichwarenprojekt/wp-license-reporter#readme",
    "keywords": [
        "license",
        "release",
        "MIT",
        "CI",
